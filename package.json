{
    "name": "momotheme",
    "displayName": "Momo Theme",
    "description": "Momo Theme - Visual Studio Code theme inspired by Material, Solarized and Boxy themes",
    "version": "0.2.1",
    "publisher": "momosct",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Themes"
    ],
    "homepage": "https://github.com/scottmo/momothemes",
    "repository": {
        "type": "git",
        "url": "https://github.com/scottmo/momothemes.git"
    },
    "bugs": {
        "url": "https://github.com/scottmo/momothemes/issues"
    },
    "scripts": {
        "build": "node ./tasks/build.js",
        "publish": "node ./tasks/build.js && vsce publish"
    },
    "contributes": {
        "themes": [
            {
                "label": "MomoLight",
                "uiTheme": "vs",
                "path": "./themes/MomoLight.json"
            },
            {
                "label": "MomoDark",
                "uiTheme": "vs-dark",
                "path": "./themes/MomoDark.json"
            },
            {
                "label": "MomoSolLight",
                "uiTheme": "vs",
                "path": "./themes/MomoSolLight.json"
            },
            {
                "label": "MomoSolDark",
                "uiTheme": "vs-dark",
                "path": "./themes/MomoSolDark.json"
            }
        ]
    },
    "dependencies": {
        "mustache": "^2.3.0"
    }
}
